{
    "name": "IPA Prototype (MSc Project)",
    "description": "This is a raw implementation of the current Interoperable Private Attribution (IPA) proposal by Mozilla and Meta. The chrome extension serves as the browser-sided API to participate in the attribution system.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js",
      "browser_action": {
        "default_title": "IPA Prototype"}
      
    },
    "permissions":
    ["storage", "activeTab", "scripting","contextMenus","tabs"],
      "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "images/ic_icon16.png",
          "32": "images/ic_icon32.png",
          "64": "images/ic_icon64.png",
          "128": "images/ic_icon128.png",
          "256": "images/ic_icon256.png"
      }
    },
      "content_scripts": [
        {
          "matches": ["https://*/*","http://*/*","file:///E:/*"],
          "js": ["contentscript.js"]
        }
      ],
    "icons": {
      "16": "images/ic_icon16.png",
      "32": "images/ic_icon32.png",
      "64": "images/ic_icon64.png",
      "128": "images/ic_icon128.png",
      "256": "images/ic_icon256.png"
    }
}
  